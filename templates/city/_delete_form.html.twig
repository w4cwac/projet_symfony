<form method="post" action="{{ path('app_city_delete', {'id': city.id}) }}" onsubmit="return confirm('Voulez-vous supprimer cette ville ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ city.id) }}"><br>
    <button class="btn btn-outline-danger">Supprimer</button>
</form>
