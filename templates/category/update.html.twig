{% extends 'base.html.twig' %}


{% block title %}Modifier une catégorie{% endblock %}

{% block body %}

<div class="container">
    <div class="form">
        <h2>Modifier une catégorie</h2><br>
        {{ form_start(form) }}
            {{ form_row(form.name,{'attr':{'class': 'form form-control'}}) }}<br>

            
            <input type='submit' value="Modifier" class="btn btn-primary">
        {{ form_end(form) }}
    </div>
</div>

{% endblock %}
